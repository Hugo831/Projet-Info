package projet;

import java.awt.*;

public class GameObjects {
    protected int posX;
    protected int posY;
    protected Graphics graphics;
    protected Panneau panneau;


    public GameObjects(int posX, int posY, Graphics graphics, Panneau panneau){
        this.posX = posX;
        this.posY = posY;
        this.graphics = graphics;
        this.panneau = panneau;

    }

    public int getPosX(){
        return posX;
    }
    public void setPosX(int newX){
        this.posX = newX;
    }
    public int getPosY(){
        return posY;
    }
    public void setPosY(int newY){
        this.posY = newY;
    }

}
